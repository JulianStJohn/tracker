{
  "name": "tracker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,tsx,env --exec \"tsx src/index.ts --no-auth\"",
    "test": "nodemon --watch src --ext ts,tsx,env --exec \"tsx src/index.ts db=tracker_test --no-auth\"",
    "prod": "nodemon --watch src --ext ts,tsx,env --exec \"tsx src/index.ts\"",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "test:playwright": "playwright test",
    "pw": "playwright test",
    "pwh": "npx playwright test --headed --workers=1 --serial"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jose": "^6.0.12",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.18.0",
    "nodemon": "^3.1.10",
    "openid-client": "^6.6.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "tsx": "^4.20.4"
  }
}
